{
  "Urls":  "http://localhost:5004",
  "ConnectionString": "Data Source=metrics.db;Version=3;",
  "TableNames": [ "cpumetrics", "dotnetmetrics" ],
  "GetMetricsInterval": "5",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
